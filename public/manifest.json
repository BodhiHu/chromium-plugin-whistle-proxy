{
  "name": "__MSG_extName__",
  "version": "2.0.7",
  "manifest_version": 2,
  "description": "__MSG_extDescription__",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "__MSG_titleAction__",
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "default_locale": "zh_CN",
  "permissions": [
    "proxy",
    "storage",
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.jd.com/*",
        "https://*.jd.hk/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}
